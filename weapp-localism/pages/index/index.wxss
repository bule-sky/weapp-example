page {
	background-color: #111302;
}

.content {
	margin-top: 30px;
}
.title,.tips {
	color: white;
}
.title {
	font-size: 36rpx;
	margin-bottom: 20px;
}
.tips {
	font-size: 30rpx;
}
.tips span {
	color: rgb(253, 203, 1);
}
.center {
	text-align: center;
}
.panel-body {
  background: rgba(255,255,255,0.7);
  padding: 15px 15px 20px;
	margin: 100px auto 0;
  width: 600rpx;
  height: auto;
  border-radius: 6px;
  position: relative;
}
.edit-ico {
	position: absolute;
	top: -40px;
	left: 50%;
	margin-top: -10px;
	margin-left: -40px;
	width: 80px;
	height: 80px;
	background-color: #347433;
	font-size: 34px;
	line-height: 80px;
	color: #fff;
	border-radius: 50%;
	background-position: center;
	background-repeat: no-repeat;
	background-image: url("data:image/jpeg;base64,");
}
.desc {
	margin-top: 30px;
	margin-bottom: 20px;
	font-size: 16px;
  line-height: 1.42857143;
  text-shadow: 0 1px 0 rgba(255,255,255,0.3);
}
.btn {
	display: block;
	margin-bottom: 0;
	font-weight: 400;
	text-align: center;
	vertical-align: middle;
	cursor: pointer;
	background-image: none;
	background-color: #347433;
	border-color: #347433;
	border: 1px solid transparent;
	white-space: nowrap;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
  padding: 10px 16px;
  font-size: 18px;
  line-height: 1.33;
  border-radius: 6px;
  color: white;
}
.start-btn {
	margin-bottom: 10px;
	color: #fdfd88;
	font-weight: bold;
}
.share-btn.other-button-hover {
	background-color: #3BB639;
	border-color: #3BB639;
}
.view-content {
	margin-top: 30px;
}
.radio-group {
	margin-top: 20px;
}
.question-title {
}
.radio-item {
  display: block;
  padding: 10px 15px;
  margin-bottom: -1px;
  background-color: #fff;
  border: 1px solid #ddd;
}
.score-title {
	color: #a94442;
	font-size: 26px;
  text-shadow: 2px 2px 2px rgba(0,0,0,0.5);
  font-weight: bold;
  padding: 8px 0;
  text-align: center;
}
.detail {
  color: #222;
  font-size: 16px;
  line-height: 32px;
  margin-bottom: 20px;
}
.detail text {
	font-weight: bold;
}
.hr {
	margin-top: 20px;
	margin-bottom: 20px;
	border: 0;
	border-top: 1px solid #eee;
}